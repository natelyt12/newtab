{
    "manifest_version": 3,
    "name": "Wibi",
    "version": "2.0",
    "description": "Trang tab mới siêu chill",
    "author": "Phúc Thanhh",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "unlimitedStorage"
    ],
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}